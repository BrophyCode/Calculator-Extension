{
    "manifest_version":3,
    "name": "Calculator Extension",
    "version": "0.1",
    "description": "Graphing calculator in Chrome Browser",
    "action": {
        "default_popup": "popup.html"
    },
    "background":{
        "service-worker": "background.js"
    },
    "options_page": "options.html",
    "permissions": ["storage"]
}